<div id="<%= dom_id food %>">
  <tr>
    <td>
      <strong>Name:</strong>
      <%= food.name %>
    </td>

    <td>
      <strong>Measurement unit:</strong>
      <%= food.measurement_unit %>
    </td>

    <td>
      <strong>tdrice:</strong>
      <%= food.price %>
    </td>

    <td>
      <strong>Quantity:</strong>
      <%= food.quantity %>
    </td>

    <td>
      <strong>User:</strong>
      <%= food.user_id %>
    </td>

    <td>
    <%= button_to "Destroy this food", food, method: :delete %>
    </td>
    
  <tr>

</div>